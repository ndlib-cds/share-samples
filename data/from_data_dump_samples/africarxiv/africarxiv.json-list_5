: "https://api.osf.io/v2/taxonomies/584240da54be81056cecaa6d/", "name": "African Studies", "@type": "subject", "parent": {"@id": "_:b56b0b09-b167-47eb-80b2-6e7d789a7205", "@type": "subject"}, "is_deleted": false}, {"@id": "_:6e5ae4a3-ee14-4604-bbd6-faa14de9d5dd", "@type": "throughsubjects", "subject": {"@id": "_:b5867e16-d963-4b12-8dd2-1824a89ff281", "@type": "subject"}, "is_deleted": false, "creative_work": {"@id": "_:67cdbfc4-4dfa-453e-a9e1-33bc6432f50d", "@type": "preprint"}}, {"@id": "_:84d2d3b4-ce54-4a84-9294-1446912d3815", "@type": "creator", "agent": {"@id": "_:658e72c6-7623-4ac1-91cf-dfa267102f2b", "@type": "person"}, "cited_as": "Dirk Kohnert", "order_cited": 0, "creative_work": {"@id": "_:67cdbfc4-4dfa-453e-a9e1-33bc6432f50d", "@type": "preprint"}}, {"@id": "_:dd8ebd13-488b-4485-8a07-a88d6888e849", "name": "royaume-uni", "@type": "tag"}, {"@id": "_:853ba3c2-f728-430c-80cd-7625b964c83f", "tag": {"@id": "_:dd8ebd13-488b-4485-8a07-a88d6888e849", "@type": "tag"}, "@type": "throughtags", "creative_work": {"@id": "_:67cdbfc4-4dfa-453e-a9e1-33bc6432f50d", "@type": "preprint"}}, {"@id": "_:86e82a17-13f1-4684-b6a4-615f865d7485", "tag": {"@id": "_:4f85108f-9bc1-4b8f-8ae7-0d1a83b59086", "@type": "tag"}, "@type": "throughtags", "creative_work": {"@id": "_:67cdbfc4-4dfa-453e-a9e1-33bc6432f50d", "@type": "preprint"}}, {"@id": "_:90a94d13-34f0-4bbe-8c3a-c82f76f0d7af", "uri": "http://dx.doi.org/10.31730/OSF.IO/BNSU3", "host": "dx.doi.org", "@type": "workidentifier", "scheme": "http", "creative_work": {"@id": "_:67cdbfc4-4dfa-453e-a9e1-33bc6432f50d", "@type": "preprint"}}, {"@id": "_:9a9a190b-9269-49d6-8a81-0b128b651042", "@type": "throughsubjects", "subject": {"@id": "_:b56b0b09-b167-47eb-80b2-6e7d789a7205", "@type": "subject"}, "is_deleted": false, "creative_work": {"@id": "_:67cdbfc4-4dfa-453e-a9e1-33bc6432f50d", "@type": "preprint"}}, {"@id": "_:e85b01a8-9235-484f-8847-ca510b946bed", "name": "post-colonialisme", "@type": "tag"}, {"@id": "_:a30f80da-f2f3-45c0-9447-1829a