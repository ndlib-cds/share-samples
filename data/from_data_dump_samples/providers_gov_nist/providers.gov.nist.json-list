ulation cells, facilitating their use in future simulations.", "identifiers": [{"@id": "_:6e081d2aaf3a4fbca400cb59f2d704d0", "@type": "WorkIdentifier"}, {"@id": "_:41730e31deb44af19a11f17afe73ed5f", "@type": "WorkIdentifier"}], "date_updated": "2015-11-19T12:30:06+00:00", "related_works": [], "related_agents": [{"@id": "_:343f45377f444e229aea9f5e27b832d9", "@type": "Creator"}, {"@id": "_:0df90d34bb1d464691f53f74e510a4e8", "@type": "Creator"}, {"@id": "_:88151b09469d42f09adcd501ae2d4992", "@type": "Creator"}, {"@id": "_:3e869a0610a6462987742dc901ad2628", "@type": "Contributor"}, {"@id": "_:abae4ccf91dd429a9a4ab42c42d1c6a4", "@type": "Contributor"}, {"@id": "_:d75fbeb8e57043a0b2a382a4dbc61280", "@type": "Contributor"}]}], "@context": {}}
